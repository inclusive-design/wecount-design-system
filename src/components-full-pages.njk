---
pagination:
    data: components.components
    size: 1
    alias: component
permalink: "components/full/{{ component.slug }}/"
title: 'Components'
---

{% extends "layout.njk" %}

{% set renderData = {title: component.title} %}

{% block head %}
    <link rel="stylesheet" href="/static/css/main.css" />
{% endblock %}

{% block inner %}
    {% include "design-system/themes.njk" %}
    {% set comp %}
      {{ c(component.name, component.context) }}
    {% endset %}
    {% include "design-system/previews/preview-" + component.preview + ".njk" %}
{% endblock %}
